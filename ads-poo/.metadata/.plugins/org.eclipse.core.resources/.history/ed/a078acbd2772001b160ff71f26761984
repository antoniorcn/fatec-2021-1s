package edu.fatec;

public class TesteStrings {
	
	public static void main(String[] args) {
		
		String a = "A";    // "A"
		String b = "B";
		String c = a;
		String d = "A";    // "A"
		String e = d + ""; // "A"
		String f = "A";
		
		if (a.equals(e)) { 
			System.out.println("A é igual a E");
		} else { 
			System.out.println("A é diferente de E");
		}
		
	}

}
